{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "camelcase": "off",
    "func-names": "off",
    "no-console": "off",
    "no-unused-vars": "off",
    "arrow-body-style": "off",
    "no-underscore-dangle": "off",
    "class-methods-use-this": "off",
    "import/extensions": "off"
  },
  "settings": {
    "import/resolver": {
      "typescript": {}
    }
  }
}
